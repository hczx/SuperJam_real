<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hc.android.superjam">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/motorcycle"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity android:name=".MainActivity"></activity>

        <activity android:name=".animoterflash.FlashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".data.file.SaveDataToFileActivity"></activity>

        <activity android:name=".data.sql.SQLiteActivity"></activity>

        <activity android:name=".customview.CustomViewActivity"></activity>
    </application>

</manifest>